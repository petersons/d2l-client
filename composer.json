{
    "name": "petersons/d2l-client",
    "description": "Client implementation for connecting to Brightspace/D2L APIs.",
    "keywords": [
        "D2L",
        "Brightspace"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Pauletich",
            "email": "antonio.pauletich95@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "illuminate/collections": "^11.3 || ^12.0",
        "illuminate/contracts": "^11.3 || ^12.0",
        "illuminate/support": "^11.3 || ^12.0",
        "nesbot/carbon": "^3.2",
        "symfony/http-client": "^7.4 || ^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.94",
        "illuminate/container": "^11.3 || ^12.0",
        "phpunit/phpunit": "^13.0"
    },
    "conflict": {
        "illuminate/conditionable": "<11.3",
        "symfony/polyfill-php83": "<1.30"
    },
    "autoload": {
        "psr-4": {
            "Petersons\\D2L\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.14.x-dev"
        },
        "laravel": {
            "providers": [
                "Petersons\\D2L\\Laravel\\ServiceProvider"
            ]
        }
    }
}

{
    "name": "petersons/d2l-client",
    "description": "Client implementation for connecting to Brightspace/D2L APIs.",
    "keywords": [
        "D2L",
        "Brightspace"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Pauletich",
            "email": "antonio.pauletich95@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "illuminate/collections": "^9.0 || ^10.0",
        "illuminate/contracts": "^9.0 || ^10.0",
        "illuminate/support": "^9.0 || ^10.0",
        "nesbot/carbon": "^2.67",
        "symfony/http-client": "^5.4.21 || ^6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.46",
        "illuminate/container": "^8.62 || ^9.0",
        "phpunit/phpunit": "^9.6.9",
        "nikic/php-parser": "^4.15.5"
    },
    "autoload": {
        "psr-4": {
            "Petersons\\D2L\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "php-cs-fixer fix",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.11.x-dev"
        },
        "laravel": {
            "providers": [
                "Petersons\\D2L\\Laravel\\ServiceProvider"
            ]
        }
    }
}

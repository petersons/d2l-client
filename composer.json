{
    "name": "petersons/d2l-client",
    "description": "Client implementation for connecting to Brightspace/D2L APIs.",
    "keywords": [
        "D2L",
        "Brightspace"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Pauletich",
            "email": "antonio.pauletich95@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-simplexml": "*",
        "illuminate/collections": "^10.0 || ^11.0 || ^12.0",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "nesbot/carbon": "^2.72 || ^3.0",
        "symfony/http-client": "^6.4 || ^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "illuminate/container": "^10.0 || ^11.0 || ^12.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Petersons\\D2L\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint": "PHP_CS_FIXER_IGNORE_ENV=1 php-cs-fixer fix",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.13.x-dev"
        },
        "laravel": {
            "providers": [
                "Petersons\\D2L\\Laravel\\ServiceProvider"
            ]
        }
    }
}
